{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.cjs.tsbuildinfo",
    "module": "CommonJS",
    "outDir": "dist/cjs",
    "esModuleInterop": true,
    "declaration": true,
    "strict": true,
    "moduleResolution": "node",
    "rootDir": "src",
    "target": "ES2020"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
